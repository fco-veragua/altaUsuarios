<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facelettaglibrary_2_0.xsd"
	version="2.0">
	<namespace>http://dual/ui</namespace>

	<tag>
		<tag-name>sovalidator</tag-name>
		<validator>
			<validator-id>sovalidator</validator-id>
		</validator>
		<attribute>
			<name>so</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	
	<tag>
		<tag-name>ramvalidator</tag-name>
		<validator>
			<validator-id>ramvalidator</validator-id>
		</validator>
		<attribute>
			<name>ram</name>
			<type>java.lang.String</type>
		</attribute>
	</tag>
	
	<tag>
		<tag-name>fechaventaconverter</tag-name>
		<converter>
			<converter-id>fechaventaconverter</converter-id>
		</converter>
	</tag>
	
	<tag>
		<tag-name>fechaventavalidator</tag-name>
		<validator>
			<validator-id>fechaventavalidator</validator-id>
		</validator>
	</tag>

</facelet-taglib>